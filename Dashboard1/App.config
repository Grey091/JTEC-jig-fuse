<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
  <appSettings>
    <!--Set worksheets collection to start from zero. Default is 0 from EPPlus version 5, 1 can be used for backward compatibility reasons -->
    <add key="EPPlus:ExcelPackage.Compatibility.IsWorksheets1Based" value="false" />
    <add key="EPPlus:ExcelPackage.LicenseContext" value="NonCommercial" />
  </appSettings>
</configuration>